<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nsu.stu.meet.dao.DictItemMapper">


    <select id="getDictItemsByType" resultType="com.nsu.stu.meet.model.DictItem">
        SELECT *
        FROM mt_dict_item
        WHERE type_id = #{typeId} AND is_enable = 1
    </select>

</mapper>
